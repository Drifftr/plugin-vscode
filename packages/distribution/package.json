{
  "name": "@ballerina/distribution",
  "version": "0.990.3-SNAPSHOT",
  "description": "",
  "main": "build/composer.js",
  "scripts": {
    "clean": "composer pkg:clean",
    "copy-theme-assets": "copyfiles -u 4 \"./node_modules/@ballerina/theme/build/assets/**\" \"./build/theme\"",
    "copy-default-theme-css": "copyfiles -u 4 \"./node_modules/@ballerina/theme/build/ballerina-*.*\" \"./build/theme\"",
    "lint": "composer pkg:lint",
    "prebuild": "npm run clean && npm run lint",
    "test": "echo \"Error: no test specified\"",
    "test-coverage": "echo \"Error: no test specified\"",
    "build": "composer pkg:build:webpack && npm run copy-theme-assets && npm run copy-default-theme-css",
    "watch": "composer pkg:watch:webpack"
  },
  "author": "ballerina.io",
  "license": "Apache-2.0",
  "dependencies": {
    "@ballerina/ballerina-by-examples": "^0.990.3-SNAPSHOT",
    "@ballerina/diagram": "^0.990.3-SNAPSHOT",
    "@ballerina/documentation": "^0.990.3-SNAPSHOT",
    "@ballerina/theme": "^0.990.3-SNAPSHOT",
    "@ballerina/tracing": "^0.990.3-SNAPSHOT",
    "@ballerina/api-designer" : "^0.990.3-SNAPSHOT"
  },
  "devDependencies": {
    "@ballerina/composer-cli": "^0.990.3-SNAPSHOT"
  }
}
